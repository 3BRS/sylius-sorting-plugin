{
	"name": "3brs/sylius-sorting-plugin",
	"type": "sylius-plugin",
	"description": "Sorting plugin for Sylius",
	"license": "MIT",
	"require": {
		"php": "^7.3|^7.4|^8.0",
		"sylius/sylius": "^1.7|^1.8|^1.9|^1.10"
	},
	"require-dev": {
		"behat/behat": "^3.4",
		"behat/mink": "^1.7@dev",
		"behat/mink-browserkit-driver": "^1.3",
		"behat/mink-extension": "^2.2",
		"behat/mink-selenium2-driver": "^1.3",
		"friends-of-behat/page-object-extension": "^0.3",
		"friends-of-behat/suite-settings-extension": "^1.0",
		"friends-of-behat/symfony-extension": "^2.0",
		"friends-of-behat/variadic-extension": "^1.1",
		"phpstan/phpstan": "0.12.42",
		"phpstan/phpstan-doctrine": "0.12.19",
		"phpstan/phpstan-symfony": "^0.12.7",
		"phpunit/phpunit": "^9.5.9",
		"sensiolabs/security-checker": "^5.0",
		"sylius-labs/coding-standard": "^3.2",
		"symfony/browser-kit": "^4.1|^5.2",
		"symfony/debug-bundle": "^4.1|^5.2",
		"symfony/dotenv": "^4.2|^5.2",
		"symfony/intl": "^4.1|^5.2",
		"symfony/web-profiler-bundle": "^4.1|^5.2",
		"symfony/web-server-bundle": "^4.1|^5.2",
		"symfony/dependency-injection": "^4.3 <4.4.19|^5.2"
	},
	"conflict": {
		"symfony/symfony": "4.1.8",
		"symfony/browser-kit": "4.1.8",
		"symfony/dependency-injection": "4.1.8",
		"symfony/dom-crawler": "4.1.8",
		"symfony/routing": "4.1.8"
	},
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"ThreeBRS\\SortingPlugin\\": "src/",
			"Tests\\ThreeBRS\\SortingPlugin\\": "tests/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/Application/Kernel.php"]
	}
}
